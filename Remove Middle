#Write your function here
def remove_middle(lst,start,end):
  secondList=lst[:start]
  #print(secondList)
  thirdList=lst[end+1:]
  #print(thirdList)
  newList=secondList+thirdList
  return newList


#Uncomment the line below when your function is done
print(remove_middle([4, 8, 15, 16, 23, 42], 1, 3))
